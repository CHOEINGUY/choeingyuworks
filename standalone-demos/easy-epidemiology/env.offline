# 비로그인 + 오프라인 모드 설정
NODE_ENV=production

# 로그인 모드 설정 (false: 로그인 불필요)
VUE_APP_REQUIRE_AUTH=false

# 오프라인 모드 활성화
VUE_APP_OFFLINE=true

# API URL 설정 (비로그인 모드에서는 사용되지 않음)
VUE_APP_API_BASE_URL=

# 기능 플래그 (비로그인 모드에서는 비활성화)
VUE_APP_ENABLE_ADMIN_PANEL=false
VUE_APP_ENABLE_DATA_SYNC=false
VUE_APP_ENABLE_USER_MANAGEMENT=false

# 로깅 설정
VUE_APP_LOG_LEVEL=ERROR 