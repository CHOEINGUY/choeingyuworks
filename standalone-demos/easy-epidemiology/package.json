{
  "name": "webpage",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "serve:noauth": "vue-cli-service serve --mode noauth",
    "build": "vue-cli-service build",
    "build:noauth": "vue-cli-service build --mode noauth",
    "build:auth": "vue-cli-service build --mode production",
    "build:clean": "node cleanup-for-production.js && vue-cli-service build",
    "build:clean:noauth": "node cleanup-for-production.js && vue-cli-service build --mode noauth",
    "build:clean:auth": "node cleanup-for-production.js && vue-cli-service build --mode production",
    "build:offline": "node scripts/embed-zip-templates.js && vue-cli-service build --mode offline",
    "build:noauth-offline": "node scripts/embed-zip-templates.js && vue-cli-service build --mode noauth",
    "cleanup": "node cleanup-for-production.js",
    "lint": "vue-cli-service lint",
    "test": "jest",
    "test:auth": "node test-modes.js auth",
    "test:noauth": "node test-modes.js noauth",
    "download-icons": "node scripts/download-material-icons.js",
    "embed-templates": "node scripts/embed-zip-templates.js",
    "postinstall": "npm run download-icons"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "@vuepic/vue-datepicker": "^11.0.2",
    "chart.js": "^4.4.8",
    "chartjs-plugin-datalabels": "^2.2.0",
    "core-js": "^3.8.3",
    "echarts": "^5.6.0",
    "flatpickr": "^4.6.13",
    "hwp.js": "^0.0.3",
    "idb-keyval": "^6.2.2",
    "immer": "^10.1.1",
    "jstat": "^1.9.6",
    "jszip": "^3.10.1",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "lodash.debounce": "^4.0.8",
    "mitt": "^3.0.1",
    "papaparse": "^5.5.2",
    "pinia": "^3.0.4",
    "tui-grid": "^4.21.22",
    "v-calendar": "^3.1.2",
    "vue": "^3.2.13",
    "vue-chartjs": "^5.3.2",
    "vue-i18n": "^9.14.5",
    "vue-router": "^4.6.4",
    "vue-toastification": "^2.0.0-rc.5",
    "vue-virtual-scroller": "^2.0.0-beta.8",
    "vuetify": "^3.7.0-beta.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@mdi/font": "^7.4.47",
    "@playwright/test": "^1.57.0",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.21",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^25.0.3",
    "@types/papaparse": "^5.5.2",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.23",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^8.0.0",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-vue": "^8.0.3",
    "jest": "^30.0.4",
    "jest-environment-jsdom": "^30.0.4",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.19",
    "ts-jest": "^29.4.6",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "vue-cli-plugin-vuetify": "~2.5.8",
    "vue-tsc": "^3.2.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "browser": true,
      "es2021": true,
      "jest": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser",
      "ecmaVersion": 2021,
      "sourceType": "module"
    },
    "globals": {
      "defineProps": "readonly",
      "defineEmits": "readonly",
      "defineExpose": "readonly",
      "withDefaults": "readonly"
    },
    "rules": {
      "no-console": "off",
      "no-debugger": "error",
      "no-unused-vars": "error",
      "prefer-const": "error",
      "no-var": "error",
      "object-shorthand": "error",
      "prefer-template": "error",
      "template-curly-spacing": "error",
      "arrow-spacing": "error",
      "comma-dangle": [
        "error",
        "never"
      ],
      "semi": [
        "error",
        "always"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "indent": [
        "error",
        2
      ],
      "vue/multi-word-component-names": "off",
      "vue/no-unused-components": "error",
      "vue/no-unused-vars": "error"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
