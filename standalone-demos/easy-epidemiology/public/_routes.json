{
  "version": 1,
  "include": ["/*"],
  "exclude": [
    "/js/*",
    "/css/*",
    "/assets/*",
    "/*.js",
    "/*.css",
    "/*.ico",
    "/*.png",
    "/*.jpg",
    "/*.jpeg",
    "/*.gif",
    "/*.svg",
    "/*.woff",
    "/*.woff2",
    "/*.ttf",
    "/*.eot",
    "/*.otf",
    "/*.zip",
    "/*.xlsx"
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate, max-age=0",
        "Pragma": "no-cache",
        "Expires": "-1"
      }
    }
  ]
} 